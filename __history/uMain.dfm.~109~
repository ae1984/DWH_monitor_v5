  object ADO_PX_USE: TADOQuery
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select '
      '    sysdate as dt'
      
        '    ,(select t.VALUE from v$px_process_sysstat t where t.STATIST' +
        'IC = '#39'Servers In Use                '#39') as px_use'
      
        '    ,(select t.VALUE from v$px_process_sysstat t where t.STATIST' +
        'IC = '#39'Servers Available             '#39') as px_not_use'
      'from dual')
    Left = 296
    Top = 72
  end
