  object ADO_CPU_UTILIZ: TADOQuery
    Active = True
    Connection = ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select '
      '   t.END_TIME as dt,t.value as val'
      '   ,(sysdate - t.END_TIME)*3600*24'
      'from v$sysmetric t '
      'where t.metric_name = '#39'Host CPU Utilization (%)'#39
      '      and (sysdate - t.END_TIME)*3600*24 <18'
      '      and rownum <= 1')
    Left = 296
    Top = 176
  end
